  "dependencies": {
    "has": "^1.0.1",
    "hast-to-hyperscript": ">=3.0.1"
  },
  "devDependencies": {
    "hastscript": "^3.0.1",
    "nyc": "^10.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "remark": "^7.0.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.6.2",
    "unified": "^6.0.0",
    "unist-builder": "^1.0.2",
    "xo": "^0.18.0"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "build-md": "remark . -qfo",
    "build": "npm run build-md",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
